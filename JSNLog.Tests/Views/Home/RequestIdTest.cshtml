@using JSNLog
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RequestIdTest</title>
    <script src="~/Scripts/jquery-2.1.0.min.js"></script>
</head>
<body>
    @Html.Raw(JSNLog.Tests.Logic.TestUtils.SetupRequestIdTest(ViewBag.PassedInRequestId, 
        @"<jsnlog productionLibraryPath=""~/Scripts/libs/jsnlog.js""></jsnlog>"))

    <div id="IdFromController">@ViewBag.RequestId</div>
    <div id="IdFromView">@JSNLog.JavascriptLogging.RequestId()</div>
    <div id="IdFromJL"></div>

    <script type="text/ecmascript">
        __jsnlog_configure = function (JL) {
            $("#IdFromJL").html(JL.requestId);
        };

        try { __jsnlog_configure(JL); } catch(e) {};
    </script>

</body>
</html>
